<h1>CV de <%= nom %></h1>

<% postes.forEach(function(postes) { %>
    <h2><%= postes.titre %> chez <%= postes.entreprise %></h2>
    <h3><%= postes.lieu %> (<%= postes.dates %>)</h3>
    <ul>
        <% postes.taches.forEach(function(taches) { %>
            <li><%= taches %></li>
        <% }); %>
    </ul>
<% }); %>